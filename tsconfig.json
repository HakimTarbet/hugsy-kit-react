{
    "compilerOptions": {
        "target": "ES5",
        "module": "CommonJS",
        "lib": ["dom", "dom.iterable", "esnext"],
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "outDir": "./dist",
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
}
  